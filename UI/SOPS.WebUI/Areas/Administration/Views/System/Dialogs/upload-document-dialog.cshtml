
<div id="upload-document-dialog" class="dialog"
    data-bind="jqDialog: { resizable: false, autoOpen: false, modal: true, width: 600, title: 'Import dokumentu' },  dialogVisible: isUploadDocumentDialogOpen">
    @using (Html.BeginForm("UploadDocument", "System", FormMethod.Post, new { enctype = "multipart/form-data", id = "UploadDocumentForm" }))
    {
        <div class="import-message"></div>
        <div>Wskaż plik któryc chcesz dodać </div>
        <input type="file" required name="file"/>
        <div class="buttons">
            <input type="submit" class="blue" value="Importuj"/>
            <input type="button" value="Anuluj" data-bind="click: closeUploadDocumentDialog"/>
        </div>
    }
</div>